            "role": "Programmer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "educoder/pest": "1.0.0",
        "devristo/phpws": "dev-master",
        "alex-sanchezzz/reactguzzle": "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "files": [
